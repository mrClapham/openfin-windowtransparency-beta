{
    "devtools_port": 9090,
    "startup_app": {
        "name": "OpenfinPOC window transparency",
        "description": "OpenFin OpenfinPOC window transparency POC",
        "url": "http://localhost:8080/",
        "__url": "http://cdn.openfin.co/demos/alpha-mask/",
        "uuid": "o",
        "autoShow": true,
        "frame": false,
        "alphaMask": {
            "red": 200,
            "green": 200,
            "blue": 200
