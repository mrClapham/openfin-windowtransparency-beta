        "installerUrl": "https://dl.openfin.co/services/download?fileName=alpha-mask-poc&config=http://cdn.openfin.co/demos/alpha-mask/app.json"
    },
    "runtime": {
        "arguments": "--v=1 -disable-legacy-window  --disable-gpu",
        "version": "4.40.3.19"
    },
    "shortcut": {
        "company": "OpenFin",
        "description": "Transparent OpenFin",
        "icon": "http://demoappdirectory.openf.in/desktop/config/apps/OpenFin/HelloOpenFin/img/openfin.ico",
        "name": "Alpha Mask"
    }
